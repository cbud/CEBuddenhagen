@misc{normanw_h_mason_etal23,
  type = {Talk},
  title = {New tools for sleeper pest response under changing climatic and social contexts},
  author = {{Norman W.H. Mason} and {Chris Buddenhagen} and {John Kean} and {Geoff Kaine}},
  year = {2023},
  month = aug,
  address = {Rotorua, New Zealand},
  abstract = {New Zealand lacks appropriate response frameworks for recent invasions where eradication is no longer considered feasible. There are {$>$}50,000 exotic species naturalised in New Zealand. Most are not currently invasive, but predicting which will become invasive, and when, is challenging. We require new tools for rapid development of management responses to these ``sleeper pests'' when they emerge as invasive threats.    Social resistance limits the success of invasive species management responses. Existing invasive species response simulation frameworks do not consider the impact of social factors on management success. Conversely, simulation approaches incorporating social factors do not include changes in habitat suitability (e.g. through climate change). We present new simulation methods capable of integrating both change in the biophysical context (e.g. climate and land use) and in the social context (e.g. public willingness to manage sleeper pests).   Two simulation functions -- INApest() and INApestMeta() - were developed to simulate pest spread and management under changing climates and in response to social interventions. The difference between them is that INApestMeta() incorporates local population dynamics in simulating pest spread and response to management. Both functions are implemented within spread models for case-study species. INApest() was implemented for Chilean needle grass (a pasture weed), while INApestMeta() was implemented for the tomato red spider mite (a horticultural pest) and for Argentine ants (a residential and horticultural pest). Selected simulation experiments are presented illustrating the ability of INApest() and INApestMeta() to compare a wide range of management and public engagement scenarios under changing climates.}
}

@misc{jessem_rubenstein_etal21,
  title = {What governs weed seed contamination in imported seed lots entering {{New Zealand}}?},
  author = {{Jesse M. Rubenstein} and {Philip E. Hulme} and {Christopher E Buddenhagen} and {M. Philip Rolston} and {John G. Hampton}},
  year = {2021},
  month = sep,
  address = {Napier, New Zealand},
  urldate = {2023-09-20},
  abstract = {Imported seed lots provide a major pathway for weed seed contaminants, and many of the weeds that are naturalised globally originated as contaminants in seed lots. Management of these trade pathways helps minimise agricultural loss and is a significant means of preventing future biological invasions. This study obtained MPI data on plant contaminants found within imported seed lots entering New Zealand from 2014--2018. In total, we examined records for 41,610 seed lots across 1,420 crop seed species. Since little is known about which variables are most associated with contaminants in seed lots entering New Zealand, examination of interception data is useful in assisting MPI with their targeted biosecurity efforts. Analysis of interception data can also be used by foreign inspection agencies to help identify levels of risk across the industry. Given that all seed lots entering New Zealand are inspected at a higher rate when compared with other foreign inspection agencies, MPI data provide a particularly complete view of the contaminants that move throughout the system. This study aims to help inform MPI and New Zealand's trading partners by determining: 1) the most commonly contaminated crop seeds and crop types entering New Zealand, including corresponding contamination rates; 2a) the most common contaminants; 2b) the most common category of contaminant; 2c) which crop types and exporting country are most commonly associated with a quarantine weed; and 3) whether size of a seed lot is linked to contamination.}
}

@misc{trevorjames_etal21,
  title = {Herbicide resistance surveys in {{New Zealand}} -- state of play 2021},
  author = {{Trevor James} and {Christopher E Buddenhagen} and {Zachary Ngow} and {M. Philip Rolston} and {Matilda Gunnarsson}},
  year = {2021},
  month = sep,
  address = {Napier, New Zealand},
  abstract = {Herbicide-resistant weeds were first detected in New Zealand in 1979. Since then, reporting has been ad hoc, relying on growers and rural professionals to recognise cases and then for researchers to have the resources to carry out experiments that confirm resistance. The commencement of an MBIE-funded programme `Managing Herbicide Resistance' in 2018 allowed the first systematic approach to survey for herbicide-resistant weeds in arable crops. The first survey in 2018/19 involved collecting seeds from weeds present in arable crops on 48 randomly selected farms, representing 20\% of arable farms in central Canterbury. These seeds were sown in pots and the seedlings were sprayed, when they had 2--4 leaves, at label rates using a range of herbicides commonly used to control that species. In 2019, 12 out of the 48 farms had weeds resistant to Group A and B herbicides. Species included ryegrass (Lolium spp.) and wild oats (Avena fatua) growing as weeds in wheat (Triticum aestivum), barley (Hordeum vulgare) and clover (Trifolium spp.) fields. A further 28 farms in south Canterbury surveyed in 2020 are expected to show similar levels of resistance. The funding has also provided resources for industry representatives and growers to send material suspected of resistance to researchers for investigation. Via word-of-mouth advertising, we received 16 self-reported samples of suspected resistant weeds (wild oats, ryegrass and chickweed (Stellaria media)) from agronomists and herbicide representatives. Ongoing research includes comprehensive dose-response experiments, heritability and the determination of the mechanisms involved i.e. target-site or nontarget-site of resistance. The work is promising to: bring the herbicide-resistance conversation into the mainstream; show the geographic extent of the problem; and reinforce the need to implement best practices that slow its development. Future surveys will capture information about resistance cases in maize farms, a crop historically known to produce cases of resistance in New Zealand.},
  collaborator = {{Richard Chynoweth} and {Hossein Ghanizadeh} and {Kerry C. Harrington}}
}

@misc{christopherebuddenhagen_etal22,
  title = {Detecting weed seeds in seed for sowing: a case study},
  author = {{Christopher E Buddenhagen} and {Ben Wynn-Jones} and {Deborah Hackell}},
  year = {2022},
  month = aug,
  address = {Christchurch, New Zealand},
  urldate = {2023-09-20},
  abstract = {Setting and following phytosanitary standards for weed seeds can lessen the impacts of weeds on farm yields. Standards adopted by seed companies, laboratories and regulators focus on ensuring the contamination rates are not higher than some threshold level. Recommended sample rates focus on the sample size needed to obtain a contaminant in a random sample of the seed lot, not detectability. To examine detectability, we had technicians examine spiked 12 samples of ca. 150,000 ryegrass and clover crop seeds with seeds from twelve non-crop species (3-8 seeds of each) and consider shapes, relative colour, relative size, texture and technician (including a commercial seed lab). Commercial lab identifications were usually correct at the level of genus, and species for common weeds, but identification errors were common. A linear model fit to the data indicated there were significant technician, crop, colour, shape and size effects on detection e.g., larger seeds were 23\% more likely to be found than smaller seeds, seeds distinctly different in colour (20\%) and distinctly different in shape 27\% were more likely to found. There was no difference between crops for seed lab detection rates, but the other technicians detected fewer seeds in ryegrass. For the regulated species Alopecurus myosuroides detection rates ranged from 7\% to 61\%. New Zealand requires a 95\% confidence that the maximum pest limit of 0.01\% of quarantine weed seed contamination is not exceeded an imported seed lot. We discuss whether this is achieved under current inspection standards.}
}

@misc{buddenhagen_etal23a,
  type = {Poster},
  title = {{{R2M}} rapid risk assessment to support integrated biosecurity responses from farm to global scales},
  author = {Buddenhagen, Chris and Mason, Norman and Choudhary, Manoj and Etherton, Berea and Kraisitudomsook, Nattapol and McCoy, Richard and Mouafo Tchinda, Romeric and Perez, Chris and Plex Sula, Aaron Isai and Garrett, Karen A.},
  year = {2023},
  month = aug,
  publisher = {AgResearch},
  address = {Christchurch, New Zealand},
  urldate = {2023-09-20},
  abstract = {The ability of national agricultural research and extension systems (NARES) and national plant protection organizations (NPPOs) to address emerging pathogen and pest risks is key to national food security. Many countries globally are in the process of improving these systems. R2M is a set of tools for rapid risk (R2) assessment and mitigation (M) planning for crop pathogens, pests and weeds at the national or regional level. These tools can help countries develop their strategies for effective management of current crop disease problems, as well as new invasive pathogens and pests. R2M tools act as building blocks in the development of a global surveillance and mitigation system for crop disease. The R2M `meta-tool' for expert knowledge elicitation provides an automated method for leaders in national programs to create their own tools for evaluating risk and mitigation strategies. R2M also includes geographic risk analysis such as cropland connectivity analysis, and management scenario analysis such as impact network analysis. We provide examples of R2M applications in several countries and introduce the latest R2M options available for new applications. It is hoped that the R2M framework will provide the basis for a global community of practice trying to solve similar problems. We discuss examples involving the following: (1) Expert elicitation. (2) Crop connectivity. (3) Regional assessments of invasive species management scenarios.},
  copyright = {Creative Commons Attribution 4.0 International},
  keywords = {Biosecurity science and invasive species ecology}
}

@misc{zacharyngow_etal22,
  type = {Talk},
  title = {Surveying for herbicide resistant weeds in {{Bay}} of {{Plenty}} and {{Waikato}} maize},
  author = {{Zachary Ngow} and {Ben Wynn-Jone} and {Trevor James} and {Chris Buddenhagen}},
  year = {2022},
  month = aug,
  abstract = {Research on maize weeds in New Zealand is extensive, but few systematic efforts were made to determine their prevalence and abundance. The most recent `weeds survey' in maize is two decades old. Weeds most problematic are those difficult to control with herbicides. The first cases of resistance were in the 1980s, Chenopodium album L. and Persicaria maculosa Gray, which both evolved resistance to atrazine. Atrazine-resistant C. album then evolved additional resistance to dicamba in the eastern Waikato in the 2000s. Recently, Digitaria sanguinalis (L.) Scop. evolved resistance to nicosulfuron in Bay of Plenty and Waikato maize farms. Apart from C. album - presumed widespread - the prevalence of resistant weeds was unknown in maize. Fifty-two maize fields in the Bay of Plenty and Waikato were sampled in late January-early February 2021 for weeds. Weed percentage cover was assessed, weed seed was collected and soil samples were taken. Weeds emerged from soil samples were identified and enumerated. One-hundred and thirty-four plant species were observed. Digitaria sanguinalis, C. album and Persicaria spp. were the most widespread weeds, observed in 98\%, 85\% and 71\% of farms, respectively. Widespread weeds abundant in fields ({$\geq$}1\% cover) were D. sanguinalis, C. album, Persicaria spp., Cyperus rotundus L. and Echinochloa crus-galli (L.) P. Beauv. In seedbanks, C. album, D. sanguinalis and Persicaria spp. were common, as well as winter/spring weeds Juncus bufonius L., Poa annua L. and Stellaria media (L.) Vill. Post-emergent herbicides were applied to C. album, D. sanguinalis and Persicaria spp. seedlings. No resistance was detected for Persicaria spp. to any herbicide, nor for Chenopodium album to dicamba, nicosulfuron and mesotrione, but 22 farms (42\%) had atrazine resistant C. album. Seven farms (13\%) had nicosulfuron resistant D. sanguinalis. Resistant D. sanguinalis survived from 60 g.ai.ha-1, twice the rate susceptible plants survived, but two populations survived 240 g.ai.ha-1. Five farms had both nicosulfuron-resistant D. sanguinalis and atrazine resistant C. album, and a total 24 farms (46\%) had a resistant weed.}
}

@misc{buddenhagen_etal23b,
  type = {Talk},
  title = {Networked socioecological models for integrating agency-led and public-led invasive species incursion responses under climate change - a regional-scale analysis of the grassland weed {$<$}em{$>$}{{Nassella}} neesiana{$<$}/em{$>$}},
  author = {Buddenhagen, Chris and Lamoureaux, Shona and Bourdot, Graeme and Garrett, Karen A. and Mason, Norman},
  year = {2023},
  month = may,
  publisher = {AgResearch},
  address = {Christchurch, New Zealand},
  urldate = {2023-09-20},
  abstract = {Invasive species incursion responses are a complex interplay of ecological processes (establishment, growth, natural and human-aided dispersal), management actions (surveillance, local eradication and spread reduction) and multiple actors (central and local government, industry bodies and private citizens). Network-based simulation is increasingly used to integrate these social and ecological processes. We present an example which builds on recent advances (specifically, the impact network analysis, INA, framework) and provides unprecedented flexibility in modelling the co-evolution of invasive species incursions and management responses. The functionality we added to INA permits inclusion of habitat (e.g., climate and land use) impacts on establishment, with potential to incorporate temporal variation in habitat suitability. There is explicit provision for separate natural and human-aided dispersal processes and temporal variation in invasion threat from external sources (e.g., national, or regional border incursions). The system separates the management response into detection, local eradication, spread reduction (e.g., restricted movement of plant material and livestock, cleaning machinery and other equipment) and management adoption components. Various incursion response strategies can be explored including sentinel sites (selecting locations of higher detection probability), preventative management in locations threatened by newly discovered incursions, enforcement of hygiene or movement-restriction measures and awareness raising in target population (e.g., farmers, seed companies, garden centres). The system retains the key functionality of INA, particularly inclusion of socioeconomic network structure in modelling information sharing (about the presence of the pest and management practices). To illustrate the potential of this approach we examine the spread of a regionally restricted harmful weed, {$<$}em{$>$}Nassella neesiana {$<$}/em{$>$}(Chilean needle grass, CNG). During seeding in late summer for three months of the year, the sharp seeds can penetrate animal hides and can be spread by livestock or contaminated equipment, so farmers typically do not graze these paddocks. CNG is regulated by three different regional authorities in New Zealand because of the risk to the sheep and beef sector. With CNG established in Hawkes Bay Region, we examine the biosecurity risk posed to the currently uninvaded adjacent region of Manawatu-Wanganui under current and future climate, and with and without management of the current infestations in Hawkes Bay. Climate change increases the area potentially impacted in Manawatu-Wanganui. With the zero-management scenario in Hawkes Bay the annual incursion rate on farms in Manawatu-Wanganui eventually exceeds 12 incursions per year, while under realistic management settings in Hawkes Bay incursions decline to 0.4 events per year.},
  copyright = {Creative Commons Attribution 4.0 International},
  keywords = {Biosecurity science and invasive species ecology}
}

