@misc{andrewgriffiths_etal23,
  type = {Talk},
  title = {A high-throughput screening tool to identify target-site herbicide resistance for three modes of action in {{{\emph{Lolium}}}} spp.},
  author = {{Andrew Griffiths} and {Hossein Ghanizadeh} and {Hayley Baird} and {Craig Anderson} and {Won Hong} and {Zachary Ngow} and {Chris Buddenhagen} and {Trevor James}},
  year = {2023},
  month = aug,
  address = {Rotorua},
  abstract = {Weeds in New Zealand's land-based production systems cause significant economic harm estimated at {\textasciitilde}NZ\$500 million per year. Herbicides are one means for mitigating weed impact, however herbicide resistance is a growing issue. Timely confirmation of resistance is required for initiating management plans and preventing its spread.  We have established a high-throughput multi-amplicon-sequencing and analysis pipeline for rapid identification of target-site resistance mutations in the ACCase, ALS and EPSPS genes targeted by Groups 1, 2 and 9 (glyphosate) herbicide modes-of-action, respectively. This `Quick' test amplifies discrete regions of these three genes in a single reaction which are then sequenced, enabling identification of variants known to confer resistance to these herbicides, and their zygosity. This method facilitates rapid screening of a range of ryegrass species including Lolium perenne, L. multiflorum, and L. hybridum and has identified resistance in ryegrass species sampled from different land-based systems. From a sample of 80 farm-derived resistant plants, we confirmed that approximately 38 had herbicide resistance-conferring mutations in the ACCase and ALS genes, and of these, 10 plants harboured resistance alleles in both genes. The method is being assessed with other weed species such as L. rigidum, Avena fatua, Phalaris spp., Poa spp., and Alopecurus myosuroides. This platform greatly simplifies testing for these common types of herbicide resistance and can be used to inform the management response. Furthermore, this technology could be used to identify and remove resistance alleles from Lolium breeding material when developing ryegrass cultivars as a way to minimise unintended spread of herbicide resistance.}
}

@misc{christopherebuddenhagen_etal23,
  type = {Talk},
  title = {Most imported and locally sourced ryegrass seed lines contain plants resistant to {{Group}} 1 or 2 herbicides},
  author = {{Christopher E Buddenhagen} and {Zachary Ngow} and {Andrew G. Griffiths} and {Hossein Ghanizadeh} and {Trevor James} and {M. Philip Rolston}},
  year = {2023},
  month = aug,
  address = {Rotorua},
  abstract = {Recent surveys revealed that {$>$}40\% of New Zealand (NZ) vineyards and arable farms contain herbicideresistant plants. Resistant ryegrass (Lolium spp.) were particularly common. Multiplication of ryegrass provided NZ \$80M in export revenues in 2021 but farmers suspected that cultivar seed-lines contained herbicide-resistant plants. We planted out seed from 56 distinct lots (28 per year) from 52 varieties, into 1.5 by 5 meter plots over two years, with four replicates per seed-lot and herbicide (336 randomized plots per year). Plots were treated with haloxyfop (Group 1), iodosulfuron (Group 2) and glyphosate at the highest recommended rate. Each herbicide was applied to approximately 30 to 80 thousand seedlings at the three-leaf stage, estimated survival rates were less than 2\%. Eight weeks after treatment, up to 20 survivors were collected per variety and herbicide, grown in the glasshouse and tillered for respraying to confirm resistance to the herbicide treatments -- because some plants could have been late germinators or somehow escaped exposure. Out of the 56 seed-lots (11 were from NZ, 45 were imported), resistance was confirmed for 44 lots, including some breeder's lines. All NZ seed lots had resistant plants. Estimated resistance rates were between 1:20 and 1:70000, with seed lots classed as resistant to either haloxyfop (9 lots), iodsulfuron (10 lots) or both (25 lots), but none survived glyphosate. Some lines contained Poa annua that survived iodosulfuron as well. We speculate that resistance-conferring variants may occur normally in ryegrass at rates of 1:7000 or rarer, but 29 seed lots had higher rates.}
}

